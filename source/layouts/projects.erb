<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "AU Design Show 2014" %></title>
    
    <%= stylesheet_link_tag "styles" %>
    <%= javascript_include_tag  "all" %>
  </head>
  
  <body class="<%= page_classes %>">
  <%= current_page.data.student %>
  <%= current_page.data.course %>
  <%= current_page.data.description %>
  <%= current_page.data.professor %>
  <%= current_page.data.semester %>
  <%= current_page.data.category %>

  <% current_page.data.images.each do |i| %>
    <img src="<%= i %>">
  <% end %>
  
  </body>
</html>