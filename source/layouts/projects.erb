<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title + " | AU Design Show 2014" %></title>
    
    <%= stylesheet_link_tag "styles" %>
    <%= javascript_include_tag  "all" %>
  </head>
  
  <body class="<%= page_classes %>">
   <nav class="m-nav">
  <a href="index.html">AU Design Show</a>

</nav> 
    <div class="work">
    <ul class="stars">
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
    </ul>

    <div class="l-wrap">
      <h2><%= current_page.data.title %></h2>
      <h3>by <%= current_page.data.student %></h3>
      <h4><%= current_page.data.course %></h4> | 
      <h4><%= current_page.data.professor %></h4> | 
      <h4><%= current_page.data.semester %></h4> | 
      <h4><%= current_page.data.category %></h4>



      <% if(current_page.data.embed)%>
        <div class="videoWrapper"><%= current_page.data.embed %></div>
      <% end %>

<div class="images">
  <% if(current_page.data.images)%>
    <% current_page.data.images.each do |i|%>
      <img src="<%= i %>">
    <% end %>
  <% end %>

    </div>
      <%= current_page.data.description %>
    <% if(current_page.data.link)%>
      <h4><a href="<%= current_page.data.link %>" class="button">View Live</a></h4>
    <% end %>
      <% if(current_page.data.portfolio)%>
        <h4><a href="<%= current_page.data.portfolio %>" class="button">Portfolio</a></h4>
      <% end %>


</div>
  </div>
  </body>
</html>